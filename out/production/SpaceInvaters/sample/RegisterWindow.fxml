<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane stylesheets="@style.css" styleClass="background" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="sample.RegisterWindowController" prefHeight="400.0" prefWidth="600.0" alignment="CENTER" hgap="10" vgap="10">
    <TextField fx:id="userName" promptText="username" GridPane.rowIndex="0"/>
    <TextField fx:id="password" promptText="password" GridPane.rowIndex="1" />
    <Button styleClass="record-sales" text="Register" GridPane.rowIndex="3" onMouseClicked="#register" />
    <Button styleClass="record-sales" text="back" GridPane.rowIndex="4" onMouseClicked="#back"/>
    <Label fx:id="registering" GridPane.rowIndex="5"/>
</GridPane>
