<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane stylesheets="@style.css" styleClass="background" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="sample.UserWindowController" prefHeight="400.0" prefWidth="600.0" alignment="center" hgap="10" vgap="10">
    <VBox alignment="CENTER" spacing="10">
        <Button styleClass="record-sales" text="Register" GridPane.rowIndex="0" onMouseClicked="#openRegisterWindow"/>
        <Button styleClass="record-sales" text="Login" GridPane.rowIndex="1" onMouseClicked="#openLoginWindow"/>
        <Button styleClass="record-sales" text="Change Username" GridPane.rowIndex="2" onMouseClicked="#openChangeUsernameWindow"/>
        <Button styleClass="record-sales" text="back" GridPane.rowIndex="3" onMouseClicked="#back"/>
    </VBox>
</GridPane>
