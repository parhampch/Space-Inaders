<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane stylesheets="@style.css" styleClass="background" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="sample.LoginWindowController" prefHeight="400.0" prefWidth="600.0" alignment="center" hgap="10" vgap="10">
    <VBox alignment="CENTER" spacing="10">
        <TextField fx:id="userName" promptText="username" GridPane.rowIndex="0"/>
        <PasswordField fx:id="password" promptText="password" GridPane.rowIndex="1"/>
        <Button styleClass="record-sales" text="login" GridPane.rowIndex="2" onMouseClicked="#login"/>
        <Button styleClass="record-sales" text="back" GridPane.rowIndex="3" onMouseClicked="#back"/>
        <Label fx:id="logged" GridPane.rowIndex="4"/>
    </VBox>
</GridPane>
